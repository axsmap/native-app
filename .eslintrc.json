{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "redux-saga"],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "global-require": "off",
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extension": "off"
  }
}
